<div>
  <%= partial 'partials/navbar' %>
</div>
<!-- Image with text and logo overlay -->
<div class="image-container" style="position: relative; max-height: 550px; overflow: hidden; align-items: center; background-size: cover; background-position: center; object-fit: cover;">
  <!-- Main Image with reduced height -->
  <%= image_tag 'projects/Stawell.jpeg', alt: 'SUPL', class: 'image', style: 'width: 100%; height: 650px; object-fit: cover;' %>

  <!-- Text overlay on the top-left with padding -->
  <div class="overlay-content" style="position: absolute; top: 2.5%; left: 5%; color: white; text-align: left;">
    <h1 class="roboto-bold" style="font-size: 6rem; color: white; margin-bottom: 0rem;">SUPL</h1>
    <p class="roboto-regular" style="font-size: 18px; color: white; margin-bottom: 0rem;">Seeks to <strong>unravel the mystery</strong> of <strong>Dark Matter</strong> using a <strong>state-of-the-art underground lab</strong> 1km below Stawell, Victoria.</p>
    <p class="roboto-regular" style="font-size: 18px; color: white; margin-bottom: 0rem;">Designed to <strong>minimize cosmic interference</strong> and <strong>join the global search</strong> for this elusive particle,</p>
    <p class="roboto-regular" style="font-size: 18px; color: white; margin-bottom: 0rem;">making it one of the <strong>most advanced labs in the southern hemisphere.</strong></p>
    <div class="button-container" style="margin-top: 1rem;">
      <a class="btn btn-ghost roboto-bold" style="color: white;" href="https://www.supl.org.au/" target="_blank">More Info</a>
    </div>
  </div>

  <!-- Brand Logo in the bottom-right corner -->
  <div class="logo-container" style="position: absolute; bottom: 5%; right: 5%; width: 150px;">
    <%= image_tag 'projects/SUPL.jpg', alt: 'SUPL Logo', class: 'image' %>
  </div>
</div>
<!-- Container for other content -->
  <div class=>
        <a href="#" class="project-image">
          <%= image_tag 'projects/ProblemDef.png', alt: 'Problem', class: 'image' %>
        </a>
    </div>

       <div>
      <a href="#" class="project-image">
        <%= image_tag 'projects/InitialResearch.png', alt: 'research', class: 'image' %>
      </a>
    </div>

    <div>
      <a href="#" class="project-image">
        <%= image_tag 'projects/UserResearch.png', alt: 'research', class: 'image' %>
      </a>
    </div>

     <div>
      <a href="#" class="project-image">
        <%= image_tag 'projects/Persona.png', alt: 'Persona', class: 'image' %>
      </a>
    </div>

     <div>
      <a href="#" class="project-image">
        <%= image_tag 'projects/HMW.png', alt: 'HMU', class: 'image' %>
      </a>
    </div>

  <!-- Additional content sections -->
  <h2 class="roboto-regular" style="color: white;">Lotus</h2>
  <h2 class="roboto-regular" style="color: white;">Prototyping</h2>
  <h1 class="roboto-regular" style="color: white;">Layout</h1>
  <h1 class="roboto-regular" style="color: white;">Smartphone App</h1>
  <h1 class="roboto-regular" style="color: white;">Website Community Hub</h1>
  <h1 class="roboto-regular" style="color: white;">AR Helmet</h1>

  <!-- Tech icons -->
  <div class="tech-icons mt-5">
    <%= image_tag 'projects/HTML5.svg', alt: 'HTML5', class: 'tech-icon' %>
    <%= image_tag 'projects/CSS3.svg', alt: 'CSS3', class: 'tech-icon' %>
    <%= image_tag 'projects/JavaScript.svg', alt: 'JavaScript', class: 'tech-icon' %>
    <%= image_tag 'projects/RubyonRails.svg', alt: 'Ruby on Rails', class: 'tech-icon' %>
    <%= image_tag 'projects/Ruby.svg', alt: 'Ruby', class: 'tech-icon' %>
  </div>

  <div class="tech-icons mt-3">
    <%= image_tag 'projects/Adobe Illustrator.svg', alt: 'Illustrator', class: 'tech-icon' %>
    <%= image_tag 'projects/Figma.svg', alt: 'Figma', class: 'tech-icon' %>
    <%= image_tag 'projects/Bootstrap.svg', alt: 'Bootstrap', class: 'tech-icon' %>
  </div>

  <!-- Button group -->
  <div class="btn-group mt-5">
    <a class="btn btn-ghost roboto-regular" href="https://www.planstonight.lol/users/sign_in" target="_blank" rel="noopener noreferrer">Link to App</a>
  </div>
</div>

<!-- Social partial -->
